## returns true if the string contains the substr provided
bool const string.contains(string substr);

## returns the size of the string
uint const string.size();

## returns a substring from the starting index
string const string.substr(const uint start);

## returns a substring from the starting index with the provided length
string const string.substr(const uint start, const uint length);

## inserts a string at the index
void string.insert(const string str, const uint index);